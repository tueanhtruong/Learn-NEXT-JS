{
  "extends": ["next/core-web-vitals", "plugin:security/recommended", "eslint:recommended"],
  "plugins": ["security"],
  "rules": {
    "react/self-closing-comp": [
      "error",
      {
        "component": true,
        "html": true
      }
    ],
    "no-unused-vars": [
      "warn",
      {
        "destructuredArrayIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ],
    "import/no-anonymous-default-export": ["off"]
  }
}
